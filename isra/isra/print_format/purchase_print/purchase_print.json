 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Purchase Order</title>\n</head>\n<body>\n    <div class=\"container\">\n        <!-- Header Section -->\n        <!--<div class=\"header\">-->\n            <div style=\"display: flex; justify-content: space-between;font-size:15px\">\n                <div class=\"company-info\">\n                    <img src=\"https://erp.israuae.com/files/WhatsApp%20Image%202025-02-14%20at%207.14.36%20PM.jpeg\" alt=\"ISRA\" width=60%>\n                    <h2><strong>Isra Trading LLC</strong></h2>\n                    <p>\n                        Warehouse No. 9, 4B Street<br>\n                        Ras Al Khor, Dubai, U.A.E<br>\n                        TRN: 100041847300003\n                    </p>\n                </div>\n                <div>\n                    <h1><strong>Purchase Order</strong></h1>\n                    <h4><strong>PO# {{ doc.name }}</strong></h4>\n                </div>\n            </div>\n        <!--</div>-->\n\n        <!-- Supplier Address and Order Details -->\n        <div class=\"header\">\n            <div style=\"display: flex; justify-content: space-between;\">\n                <div class=\"company-info\">\n                    <h2><strong>Supplier</strong></h2>\n                   <p style=\"font-size:15px\">{{ doc.shipping_address_display }}</p>\n                </div>\n                <div style=\"display: flex; justify-content: space-between; gap:10px;font-size:15px\">\n                    <div style=\"text-align: right;\">\n                        <p>Bill Date :</p>\n                        <p>Due Date :</p>\n                        <p>Terms :</p>\n                    </div>\n                    <div>\n                        <p>{{ doc.transaction_date }}</p>\n                        <p>{{ doc.schedule_date }}</p>\n                        <p>Due on Reciept</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <!-- Goods Description -->\n        <table class=\"details\" width=\"100%\" style=\"font-size:15px\">\n            <tr style=\"background-color:black;color:white\">\n                <!--<th>Sr</th>-->\n                <th>#</th>\n                <th>Item & Description</th>\n                <th>Qty</th>\n                <th>Rate</th>\n                <th>Tax</th>\n                <th>Amount</th>\n            </tr>\n            {%- for row in doc.items -%}\n            <tr style=\"border-bottom: 1px solid;\">\n                <td class=\"sr\">{{ row.idx }}</td>\n                <td>{{ row.description }}</td>\n                <td>\n                    {{ \"{:.2f}\".format(row.qty) }}<br>\n                    <div style=\"font-size:13px\">{{ row.uom }}</div>\n                </td>\n                <td>{{ \"{:.2f}\".format(row.rate) }}</td>\n                <td>\n                    {{ \"{:.2f}\".format(row.amount * ( 5 / 100)) }}<br>\n                    5.00%\n                </td>\n                <td>{{ \"{:.2f}\".format(row.amount) }}</td>\n            </tr>\n            {%- endfor -%}\n            <tr style=\"border-bottom: 1px solid;\">\n                <td></td>\n                <td></td>\n                <td colspan=2 style=\"text-align:center;\"><strong>Sub Total</strong></td>\n                <td>\n                    {% set total_tax_amount = doc.items | sum(attribute='amount') %}\n                    {{ \"{:.2f}\".format(total_tax_amount * 0.05) }}\n                </td>\n                <td>\n                    {% set total_amount = doc.items | sum(attribute='amount') %}\n                    {{ \"{:.2f}\".format(total_amount) }}\n                </td>\n            </tr>\n            <tr>\n                <td></td>\n                <td></td>\n                <td></td>\n                <td colspan=2 style=\"text-align:center;\"><strong>Total</strong></td>\n                <td>\n                    {{doc.currency}} {{ \"{:.2f}\".format(doc.grand_total) }}\n                </td>\n            </tr>\n        </table>\n    \n    <footer style=\"position: fixed; bottom: 0; width: 100%; text-align: center; font-size: 18px;\">\n    <hr>\n    <p>\n        +971 42868188 | operations@alisra.co | www.israuae.com<br>\n        P.O. Box: 300058 | Warehouse no. 9, Ras Al Khor, Industrial Area 2, Dubai, UAE\n    </p>\n    </footer>\n</body>\n</html>\n",
